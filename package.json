{
	"name": "seox",
	"version": "1.1.0",
	"publishConfig": {
		"access": "public"
	},
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"bin": {
		"seox": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"import": "./dist/next.js",
			"require": "./dist/next.cjs"
		}
	},
	"type": "module",
	"author": "Kyllian SENRENS <kylliansenrens3004@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neysixx/seox.git"
	},
	"scripts": {
		"prepare": "husky",
		"format:check": "biome check",
		"format:fix": "biome check --write",
		"build": "NODE_ENV=production tsup",
		"build:dev": "NODE_ENV=development tsup",
		"build:watch": "NODE_ENV=development tsup --watch",
		"build:analyze": "ANALYZE=true NODE_ENV=production tsup",
		"analyze": "node scripts/analyze-build.js",
		"dev": "NODE_ENV=development tsup --watch",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "bun run build && bun run test",
		"ci": "bun run typecheck && bun run format:check && bun run test && bun run build"
	},
	"keywords": [
		"nextjs",
		"seo",
		"typescript"
	],
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "latest",
		"husky": "^9.1.7"
	},
	"dependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/fs-extra": "^11.0.4",
		"@types/prompts": "^2.4.9",
		"chalk": "^5.3.0",
		"commander": "^11.1.0",
		"fs-extra": "^11.2.0",
		"install": "^0.13.0",
		"ora": "^7.0.1",
		"prompts": "^2.4.2",
		"semantic-release": "^24.2.9",
		"tsup": "^8.5.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.3",
		"next": ">=14.0.0",
		"react": ">=18.0.0"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": false
		},
		"react": {
			"optional": false
		}
	},
	"engines": {
		"node": ">=18.0.0",
		"bun": ">=1.0.0"
	}
}
